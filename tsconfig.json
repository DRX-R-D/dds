{
  "compilerOptions": {
    "sourceMap": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "outDir": "./dist",
    "jsx": "preserve",
    "jsxImportSource": "@emotion/react",
    "module": "CommonJS",
    "target": "ESNext",
    "declaration": true,
    "strict": true,
    "typeRoots": ["index.d.ts", "node_modules/@types"]
  },
  "include": [
    "./src/**/*.ts"
  ],
  "exclude": [
    "node_modules",
  ]
}
